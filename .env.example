# Google Account Automation System Configuration

# Rate Limiting
ACCOUNTS_PER_DAY=100
ACCOUNTS_PER_HOUR=10

# Worker Configuration
MAX_CONCURRENT_WORKERS=5

# Storage Configuration
STORAGE_PROVIDER=database
DATABASE_URL=sqlite:./accounts.db
ENCRYPTION_KEY=your-secure-encryption-key-here

# Cloudflare Configuration (for KV storage and Workers)
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token
CLOUDFLARE_KV_NAMESPACE_ID=your-kv-namespace-id

# Temporary Email Service
TEMP_EMAIL_API_KEY=your-temp-email-api-key
TEMP_EMAIL_BASE_URL=https://api.tempmail.org

# VPN Configuration (optional)
VPN_PROVIDER=protonvpn
VPN_USERNAME=your-vpn-username
VPN_PASSWORD=your-vpn-password

# Monitoring and Alerting
LOG_LEVEL=info
ENABLE_METRICS=true
ALERT_EMAIL=admin@example.com
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url

# Proxy Configuration (optional)
PROXY_SERVER=http://proxy.example.com:8080
PROXY_USERNAME=proxy-user
PROXY_PASSWORD=proxy-pass